<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="form.type.book" class="Acme\TestBundle\Form\Type\BookType">
            <tag name="form.type" alias="book" />
        </service>
        <service id="form.type.post" class="Acme\TestBundle\Form\Type\PostType">
            <tag name="form.type" alias="post" />
        </service>
        <service id="form.type.tags" class="Acme\TestBundle\Form\Type\TagsType">
            <tag name="form.type" alias="tags" />
        </service>
    </services>

</container>